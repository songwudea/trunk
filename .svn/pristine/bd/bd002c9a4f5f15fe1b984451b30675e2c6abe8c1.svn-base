package com.babailiren.ec.model;

import java.util.Date;

/**
 * 评论
 *
 */
public class Comment implements java.io.Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	/** 流水号* */
	private Integer id;

	/** 商品ID* */
	private Integer itemId;
	
	private String commentType;
	
	private String itemName;
	
	private Integer contentId;
	
	/** 用户ID* */
	private Integer userId;

	/** 用户名称* */
	private String userName ;

	/** 评论名称* */
	private String title;

	/** 优点内容* */
	private String commentGood;

	/** 缺点内容* */
	private String commentBad;

	/** 使用心得* */
	private String commentUseExp;

	/** 评论者IP* */
	private String commentIp;

	/** 是否显示* */
	private boolean commentDisplayFlag;

	/** 创建时间* */
	private Date createdAt;
	
	private String createdBy;
	
	private String endcreatedAt;
	
	private String startcreatedAt;

	/** 用户匿名显示 * */
	private boolean commentAnonymous;
	
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getItemId() {
		return itemId;
	}

	public void setItemId(Integer itemId) {
		this.itemId = itemId;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCommentGood() {
		return commentGood;
	}

	public void setCommentGood(String commentGood) {
		this.commentGood = commentGood;
	}

	public String getCommentBad() {
		return commentBad;
	}

	public void setCommentBad(String commentBad) {
		this.commentBad = commentBad;
	}

	public String getCommentUseExp() {
		return commentUseExp;
	}

	public void setCommentUseExp(String commentUseExp) {
		this.commentUseExp = commentUseExp;
	}

	public String getCommentIp() {
		return commentIp;
	}

	public void setCommentIp(String commentIp) {
		this.commentIp = commentIp;
	}

	public boolean isCommentDisplayFlag() {
		return commentDisplayFlag;
	}

	public void setCommentDisplayFlag(boolean commentDisplayFlag) {
		this.commentDisplayFlag = commentDisplayFlag;
	}

	public Integer getContentId() {
		return contentId;
	}

	public void setContentId(Integer contentId) {
		this.contentId = contentId;
	}

	public Date getCreatedAt() {
		return createdAt;
	}

	public void setCreatedAt(Date createdAt) {
		this.createdAt = createdAt;
	}

	public String getCreatedBy() {
		return createdBy;
	}

	public void setCreatedBy(String createdBy) {
		this.createdBy = createdBy;
	}

	public boolean isCommentAnonymous() {
		return commentAnonymous;
	}

	public void setCommentAnonymous(boolean commentAnonymous) {
		this.commentAnonymous = commentAnonymous;
	}

	public String getItemName() {
		return itemName;
	}

	public void setItemName(String itemName) {
		this.itemName = itemName;
	}

	public String getCommentType() {
		return commentType;
	}

	public void setCommentType(String commentType) {
		this.commentType = commentType;
	}

	public String getEndcreatedAt() {
		return endcreatedAt;
	}

	public void setEndcreatedAt(String endcreatedAt) {
		this.endcreatedAt = endcreatedAt;
	}

	public String getStartcreatedAt() {
		return startcreatedAt;
	}

	public void setStartcreatedAt(String startcreatedAt) {
		this.startcreatedAt = startcreatedAt;
	}
	
	
	
}
