    <c:if test="${null ne errorMessage and '' ne errorMessage}">
   	<div class="error">${errorMessage}</div>
   	</c:if>
    <c:if test="${null ne infoMessage and '' ne infoMessage}">
   	<div class="info">${infoMessage}</div>
   	</c:if>
    <c:if test="${null ne noticeMessage and '' ne noticeMessage}">
   	<div class="notice">${noticeMessage}</div>
   	</c:if>
    <c:if test="${null ne successMessage and '' ne successMessage}">
   	<div class="success">${successMessage}</div>
   	</c:if>